<div class="container">
  <div class="row">
    <div class="col-md-10 offset-md-1">
    <h2>掲示板</h2>
      <div class="col-md-8 offset-md-1">
        <thead>
          <tr>
          <th>タイトル</th>
          <th>本文</th>
          <th colspan = "2"></th>
          </tr>
        </thead>
        <tbody>
      　<% @tweets.each do |tweet| %>
        <tr>
         <td><%=  link_to tweet.title, admins_tweet_path(tweet.id) %></td>
         <td><%= tweet.body %></td>
        </tr>
        <% end %>
        </tbody>
      </div>
    </div>
  </div>
</div>